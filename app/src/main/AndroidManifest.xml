<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.msgshareapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_shareapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_shareapp_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MsgShareApp"
        tools:targetApi="31">


        <activity
            android:name=".Acitivity.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MsgShareApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Acitivity.SecondActivity"/>
        <activity android:name=".Acitivity.HobbiesActivity"/>
    </application>

</manifest>